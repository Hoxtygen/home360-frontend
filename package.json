{
  "name": "next-fullstack-project",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "release": "release-it",
    "test:integration": "dotenv -e .env.test.local -- jest -i",
    "test:unit": "jest --watch",
    "migrate:test": "dotenv -e .env.test.local -- npx prisma migrate deploy"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@changesets/cli": "^2.26.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "@radix-ui/react-dropdown-menu": "^2.0.3",
    "@radix-ui/react-select": "^1.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^8.6.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.4.0",
    "cloudinary": "^1.34.0",
    "clsx": "^1.2.1",
    "datauri": "^4.1.0",
    "date-fns": "^2.29.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "form-data": "^4.0.0",
    "formidable": "3.2.5",
    "jest": "^29.5.0",
    "joi": "^17.8.3",
    "jose": "^4.12.2",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.118.0",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "next": "13.2.1",
    "next-api-route-middleware": "^1.0.2",
    "next-auth": "^4.19.2",
    "next-connect": "^0.13.0",
    "pg": "^8.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.10.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.0",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.2",
    "autoprefixer": "^10.4.13",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prisma": "^4.10.1",
    "release-it": "^15.6.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.(tsx|ts)": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "refactor",
              "section": "Refactor"
            },
            {
              "type": "test",
              "section": "Test"
            },
            {
              "type": "chore",
              "section": "Chore"
            }
          ]
        }
      }
    }
  }
}
